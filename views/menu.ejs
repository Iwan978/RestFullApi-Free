<!DOCTYPE html>
<html lang="en">
<head>
    <title>NueApi</title>
    <link rel="icon" type="image/png" href="https://telegra.ph/file/e07e0fddc2a2967bd08d6.jpg"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Rest apis WhatsApp bot, Telegram bot, and Discord bot, free, has AI download IG feature and others">
    <meta name="tag" content="free api, nue-api, rest-api, azzapi">
    <style>
        body {
            background-color: #121212;
            color: #f8f9fa;
            font-family: Arial, sans-serif;
            margin: 0;
        }

        .container {
            display: flex;
            justify-content: center;
            align-items: center;
            min-height: 100vh;
            padding: 20px;
        }

        .menu-container {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 20px;
        }

        .menu-item {
            background-color: rgba(0, 0, 0, 0.5);
            border: 1px solid #343a40;
            padding: 20px;
            border-radius: 8px;
            text-align: center;
        }

        h1, h2, p {
            text-align: center;
        }

        .input-field {
            width: calc(100% - 20px);
            padding: 10px;
            margin: 10px 0;
            border: 1px solid #343a40;
            border-radius: 4px;
            background-color: #2c2c2c;
            color: #f8f9fa;
        }

        .input-field:focus {
            border-color: #007bff;
            outline: none;
        }

        .button {
            display: block;
            width: 100%;
            padding: 10px;
            margin-top: 10px;
            border: none;
            color: #f8f9fa;
            background-color: #007bff;
            cursor: pointer;
            border-radius: 4px;
            transition: background-color 0.3s ease;
        }

        .button:disabled {
            background-color: #6c757d;
            cursor: not-allowed;
        }

        .button:hover:enabled {
            background-color: #0056b3;
        }

        .home-button {
            position: fixed;
            top: 20px;
            left: 20px;
            background: transparent;
            border: none;
            cursor: pointer;
        }

        .home-button img {
            width: 40px;
            height: 40px;
        }

        #search-bar {
            width: 100%;
            padding: 10px;
            margin-bottom: 20px;
            border: 1px solid #343a40;
            border-radius: 4px;
            background-color: #2c2c2c;
            color: #f8f9fa;
        }
    </style>
</head>
<body>
    <a href="/dashboard" class="home-button">
        <img src="https://static.vecteezy.com/system/resources/previews/024/029/940/original/home-icon-clipart-transparent-background-free-png.png" alt="Home">
    </a>


    <div class="container">
        <div class="menu-container" id="menu-container">
<input type="text" id="search-bar" placeholder="Cari menu...">
            </div>
    </div>
    

    <script>
        document.getElementById('search-bar').addEventListener('input', function() {
            const searchTerm = this.value.toLowerCase();
            renderMenuItems(searchTerm);
        });

        const menuItems = [
            {
                title: 'AI Logic',
                description: 'Buat AI Anda sendiri dengan model Gemma2 dengan sesi chat',
                inputPlaceholder: 'Siapa kamu?',
                endpoint: '/api/lgpt?user=Gapur&systemPrompt=anda adalah NueAI asisten cerdas yang dapat membantu apa saja berbasis teks, anda masih dalam tahap pengembangan dan pengembangan ini di pimpin oleh NueApi&text=',
                inputId: 'alicia-text'
            },
            {
                title: 'Anime Reaction',
                description: 'Dapatkan reaksi anime',
                endpoint: '/api/anime-reaction',
                isButtonOnly: true 
            },
            {
                title: 'Anime Schedule',
                description: 'Jadwal anime berdasarkan hari',
                inputPlaceholder: 'Masukkan hari',
                endpoint: '/api/anime-jadwal?hari=',
                inputId: 'anime-day'
            },
            {
                title: 'Calendar Events',
                description: 'Lihat daftar acara kalender',
                endpoint: '/api/acara',
                isButtonOnly: true
            },
            {
                title: 'ChatGPT',
                description: 'Model GPT-3',
                inputPlaceholder: 'Masukkan perintah',
                endpoint: '/api/gpt?model=gpt-3.5-turbo&prompt=',
                inputId: 'chatgpt-prompt'
            },
            {
                title: 'Date Now',
                description: 'Waktu, tanggal, dan acara mendatang',
                endpoint: '/api/date?zone=Asia/Jakarta',
                isButtonOnly: true 
            },
            {
                title: 'FB & IG Downloader',
                description: 'Download video/foto dari Instagram dan Facebook',
                inputPlaceholder: 'Masukkan URL Instagram/Facebook',
                endpoint: '/api/snapsave?url=',
                inputId: 'instagram-url'
            },
            {
                title: 'Gemini (Scraped)',
                description: 'Gemini hasil scraping dari gemini.google.com (tidak resmi)',
                inputPlaceholder: 'Masukkan perintah',
                endpoint: '/api/bard?text=',
                inputId: 'gemini-text'
            },
            {
                title: 'Gemini Model',
                description: 'Model Gemini Flash dari Google AI',
                inputPlaceholder: 'Masukkan perintah',
                endpoint: '/api/gemini?prompt=',
                inputId: 'gemini-model-prompt'
            },
            {
                title: 'Google Search',
                description: 'Cari di Google',
                inputPlaceholder: 'Masukkan kata kunci',
                endpoint: '/api/google?limit=3&query=',
                inputId: 'google-query'
            },
            {
                title: 'Image Search',
                description: 'Cari gambar di internet',
                inputPlaceholder: 'Masukkan kata kunci',
                endpoint: '/api/image?query=',
                inputId: 'image-query'
            },
            {
                title: 'Info Gempa (BMKG)',
                description: 'Informasi gempa bumi terbaru dari BMKG',
                endpoint: '/api/gempa',
                isButtonOnly: true
            },
            {
                title: 'NueAI (with Sessions)',
                description: 'Berinteraksi dengan NueAI (Model Gemma2 dengan sesi chat)',
                inputPlaceholder: 'Masukkan perintah',
                endpoint: '/api/sgpt?user=yanto&text=',
                inputId: 'sgpt-text'
            },
            {
                title: 'Stable Diffusion',
                description: 'AI yang dapat menghasilkan gambar dari teks',
                endpoint: 'https://tattered-classy-comic.glitch.me/sdlist',
                isButtonOnly: true 
            },
            {
                title: 'Stable Diffusion XL',
                description: 'AI yang dapat menghasilkan gambar dari teks dengan kualitas lebih tinggi',
                endpoint: 'https://tattered-classy-comic.glitch.me/sdxllist',
                isButtonOnly: true 
            },
            {
                title: 'Text To Speech',
                description: 'Ubah teks menjadi audio',
                inputPlaceholder: 'Masukkan teks',
                endpoint: '/api/tts?lang=id&text=',
                inputId: 'tts-text'
            },
            {
                title: 'TikTok Downloader',
                description: 'Download video, foto, dan musik dari TikTok',
                inputPlaceholder: 'Masukkan URL TikTok',
                endpoint: '/api/tt-dl?url=',
                inputId: 'tiktok-url'
            },
            {
                title: 'Upscale Image',
                description: 'Upscale gambar dari URL',
                inputPlaceholder: 'Masukkan URL gambar',
                endpoint: '/api/upscale?url=',
                inputId: 'upscale-url'
            },
            {
                title: 'YouTube Downloader',
                description: 'Download video/audio dari YouTube',
                inputPlaceholder: 'Masukkan URL YouTube',
                endpoint: '/api/ytdl?url=',
                inputId: 'ytdl-url'
            },
            {
                title: 'YouTube Player',
                description: 'Cari dan putar video/audio YouTube',
                inputPlaceholder: 'Masukkan kata kunci YouTube',
                endpoint: '/api/play?query=',
                inputId: 'youtube-query'
            },
            {
                title: 'YouTube Search',
                description: 'Cari video di YouTube',
                inputPlaceholder: 'Masukkan kata kunci',
                endpoint: '/api/yt-search?query=',
                inputId: 'yt-search-query'
            }
        ];

        function renderMenuItems(searchTerm = '') {
            const menuContainer = document.getElementById('menu-container');
            menuContainer.innerHTML = ''; 

            const filteredMenuItems = menuItems
                .filter(item => item.title.toLowerCase().includes(searchTerm))
                .sort((a, b) => a.title.localeCompare(b.title));

            filteredMenuItems.forEach(item => {
                const menuItemElement = document.createElement('div');
                menuItemElement.classList.add('menu-item');

                const titleElement = document.createElement('h2');
                titleElement.textContent = item.title;
                menuItemElement.appendChild(titleElement);

                const descriptionElement = document.createElement('p');
                descriptionElement.textContent = item.description;
                menuItemElement.appendChild(descriptionElement);

                if (!item.isButtonOnly) {
                    const inputElement = document.createElement('input');
                    inputElement.type = 'text';
                    inputElement.classList.add('input-field');
                    inputElement.id = item.inputId;
                    inputElement.placeholder = item.inputPlaceholder;
                    inputElement.required = true;
                    menuItemElement.appendChild(inputElement);

                    const buttonElement = document.createElement('button');
                    buttonElement.classList.add('button');
                    buttonElement.textContent = 'Try Now';
                    buttonElement.disabled = true; 
                    buttonElement.addEventListener('click', () => tryNow(item.endpoint, item.inputId));
                    menuItemElement.appendChild(buttonElement);

                    inputElement.addEventListener('input', () => {
                        buttonElement.disabled = !inputElement.value;
                    });
                } else {
                    const buttonElement = document.createElement('button');
                    buttonElement.classList.add('button');
                    buttonElement.textContent = 'Try Now';
                    buttonElement.addEventListener('click', () => {
                        if (item.endpoint.startsWith('http')) {
                            window.location.href = item.endpoint; 
                        } else {
                            tryNow(item.endpoint, item.inputId); 
                        }
                    });
                    menuItemElement.appendChild(buttonElement);
                }
                menuContainer.appendChild(menuItemElement);
            });
        }

        function tryNow(endpoint, inputId) {
            const inputValue = document.getElementById(inputId).value;
            if (inputValue) {
                location.href = `${endpoint}${encodeURIComponent(inputValue)}`;
            }
        }

        renderMenuItems();
    </script>
</body>
</html>